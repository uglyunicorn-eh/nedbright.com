{
  "extends": "astro/tsconfigs/strict",
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "functions/**/*"
  ],
  "compilerOptions": {
    "module": "esnext",
    "baseUrl": ".",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      },
    ],
  }
}
